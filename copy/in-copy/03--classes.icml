  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Bold">
    <Content>6 - Cleric: Life Domain Spells</Content>
  </CharacterStyleRange>
</ParagraphStyleRange>
